class model_data : public ad_comm{
  public:
  ofstream *   pad_evalout;
  data_int  styr;
  data_int  endyr;
  ivector yrs;
  data_int  num_indx;
  data_int  n_PE;
  data_ivector PE_vec;
  data_int  nobs;
  data_ivector yrs_srv;
  data_matrix srv_est;
  data_matrix srv_cv;
